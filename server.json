{
    "trayicon":"apple-touch-icon.png",
    "name":"cfdocs",
    "web":{
        "rewrites":{
            "config":"rewrites.xml",
            "enable":"true"
        },
        "http":{
            "port":8411
        }
    },
    "app":{
        "cfengine":"lucee-light@5.3.7+47"
    },
    "JVM":{
        "minHeapSize":"256m",
        "heapSize":"512m",
        "args":"-XX:+HeapDumpOnOutOfMemoryError -XX:HeapDumpPath=/tmp/cfdocs_heap_dump.hprof"
    }
}
